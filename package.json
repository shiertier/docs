{
  "name": "wsl-docs-gitbook",
  "version": "0.1.0",
  "private": true,
  "description": "GitBook/HonKit site generator for wsl-docs with wiki graph navigation",
  "scripts": {
    "gitbook:prepare": "node gitbook/prepare.mjs",
    "gitbook:build": "npm run gitbook:prepare && honkit build .tmp/gitbook-src .tmp/gitbook-dist",
    "gitbook:serve": "npm run gitbook:prepare && honkit serve .tmp/gitbook-src",
    "gitbook:clean": "rm -rf .tmp/gitbook-src .tmp/gitbook-dist"
  },
  "devDependencies": {
    "gitbook-plugin-relation-graph": "file:gitbook/plugins/relation-graph",
    "honkit": "^6.0.0"
  }
}
